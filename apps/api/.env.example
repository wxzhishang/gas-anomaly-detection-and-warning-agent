# 数据库配置
DATABASE_URL=postgresql://postgres:password@localhost:5432/gas_regulator

# Redis配置
REDIS_URL=redis://localhost:6379

# LLM配置
# API Key
GAS_LLM_API_KEY=your_api_key_here

# API Base URL (可选，用于兼容OpenAI API的其他服务)
# 示例：
# - OpenAI官方: https://api.openai.com/v1 (默认，可不填)
# - SiliconFlow: https://api.siliconflow.cn/v1
# - 其他兼容服务的API地址
GAS_LLM_BASE_URL=

# 模型名称
# OpenAI官方模型: gpt-3.5-turbo, gpt-4, gpt-4-turbo-preview
# SiliconFlow模型: deepseek-ai/DeepSeek-V3, Qwen/Qwen2.5-7B-Instruct
# 其他服务的模型名称
GAS_LLM_MODEL=gpt-3.5-turbo

# 服务器配置
PORT=3001
NODE_ENV=development

# 日志配置
LOG_LEVEL=info

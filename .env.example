# 根目录 .env.example 文件
# 这个文件主要用于 Docker Compose 和脚本使用

# Database Configuration
POSTGRES_DB=gas_regulator
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_PORT=5432

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# LLM Configuration (Docker 环境使用)
GAS_LLM_API_KEY=your_api_key_here
GAS_LLM_BASE_URL=https://api.openai.com/v1
GAS_LLM_MODEL=gpt-4

# API Configuration
API_PORT=3001
NODE_ENV=development

# Web Configuration
WEB_PORT=3000
NEXT_PUBLIC_API_URL=http://localhost:3001
NEXT_PUBLIC_WS_URL=ws://localhost:3001
